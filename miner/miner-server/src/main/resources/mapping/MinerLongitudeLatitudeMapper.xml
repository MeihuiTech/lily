<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mei.hui.miner.mapper.MinerLongitudeLatitudeMapper">

    <!--查询fil矿工id节点地图-->
    <select id="selectMinerLongitudeLatitudeVOList"
            resultType="com.mei.hui.miner.feign.vo.MinerLongitudeLatitudeVO">
        SELECT
        miner_id minerId,ip,address,latitude,longitude,type
        FROM
        miner_longitude_latitude
        ORDER BY type
    </select>

</mapper>